---
export interface Props {
  title: string;
  url: string;
}

const { title, url } = Astro.props;
---

<iframe
  class="mx-auto"
  width="560"
  height="315"
  src={url}
  title={title}
  allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
  allowfullscreen></iframe>
